<div class="container">
	<div class="card">
	  <div class="card-header">
	    Article #<%= @article.id %>
	  </div>
	  <div class="card-body">
	    <h5 class="card-title"><%= @article.title %></h5>
	    <p class="card-text"><%= @article.content %></p>
	  </div>
	  <%= link_to articles_path do %>
	  	<div class="btn btn-primary">Back to articles</div>
	  <%end%>  
	</div>
</div>